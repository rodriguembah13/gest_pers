{% extends 'base.html.twig' %}

{% block title %}Entreprise{% endblock %}
{% block page_title %} Entreprise {% endblock %}
{% block page_subtitle %} Detail {% endblock %}
{% block page_content %}

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entreprise.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entreprise.libelle }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entreprise.adresse }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ entreprise.telephone }}</td>
            </tr>
        </tbody>
    </table>

    <div class="col-md-9"> <a class="btn btn-warning" href="{{ path('entreprise_index') }}">back to list</a>

        <a class="btn btn-primary" href="{{ path('entreprise_edit', {'id': entreprise.id}) }}">edit</a></div>
    <div class="col-md-3"> {{ include('entreprise/_delete_form.html.twig') }}</div>
{% endblock %}
