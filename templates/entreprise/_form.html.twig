{{ form_start(form) }}
<div class="row">

<div class="col-md-6">
    {{ form_row(form.libelle) }}
    {{ form_row(form.pays) }}
    {{ form_row(form.telephone) }}
    {{ form_row(form.capital) }}
    {{ form_row(form.dateAutorisation) }}
    {{ form_row(form.email) }}
</div>
<div class="col-md-6">
    {{ form_row(form.adresse) }}
    {{ form_row(form.ville) }}
    {{ form_row(form.directeur) }}
    {{ form_row(form.autorisation) }}
    {{ form_row(form.fax) }}
</div>
</div>
    {{ form_widget(form) }}
<div class="pull-right">
    <button type="reset" class="btn btn-warning "><i class="fa fa-times"></i> {{ 'Cancel'|trans }}</button>
    <button type="submit" class="btn btn-primary "><i class="fa fa-check-square"></i> {{ button_label|default('Save') }}</button>
</div>


{{ form_end(form) }}
