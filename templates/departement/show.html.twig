{% extends 'base.html.twig' %}

{% block title %}Departement{% endblock %}
{% block page_title %} Departement {% endblock %}
{% block page_subtitle %} Detail {% endblock %}
{% block page_content %}

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ departement.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ departement.libelle }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ departement.code }}</td>
            </tr>
        </tbody>
    </table>

    <div class="row">


        <div class="col-md-9"> <a class="btn btn-warning" href="{{ path('departement_index') }}">back to list</a>

            <a class="btn btn-primary" href="{{ path('departement_edit', {'id': departement.id}) }}">edit</a></div>
        <div class="col-md-3"> {{ include('departement/_delete_form.html.twig') }}</div>
    </div>
{% endblock %}
